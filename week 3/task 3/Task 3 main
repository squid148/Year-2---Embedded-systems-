#include "mbed.h"
#include "arm_book_lib.h"



DigitalIn enterButton(BUTTON1);
DigitalIn sw1_gas_tog(D2);
DigitalIn sw2_gas_state(D3);
DigitalIn sw3_Otemp_state(D4);
DigitalIn sw4_Otemp_tog(D5);
DigitalIn alarm_ar(D6);
DigitalIn mon_tog(D7);

void Initialize_inputs () {
sw1_gas_tog.mode(PullDown)
sw2_gas_state.mode(PullDown)
sw3_Otemp_state.mode(PullDown)
sw4_Otemp_tog.mode(PullDown)
alarm_ar.mode(PullDown)
mon_tog.mode(PullDown)
}

